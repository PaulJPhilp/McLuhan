{
  "name": "effect-ai-sdk",
  "version": "0.1.0",
  "description": "Effect-native wrapper around Vercel AI SDK v5 for type-safe AI operations",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.1.33",
  "repository": {
    "type": "git",
    "url": "https://github.com/PaulJPhilp/mcluhan.git",
    "directory": "packages/effect-ai-sdk"
  },
  "license": "MIT",
  "author": "Paul J Philp",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check .",
    "format": "biome check .",
    "format:fix": "biome format --write ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.18"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/deepseek": "^0.2.16",
    "@ai-sdk/google": "^1.2.22",
    "@ai-sdk/groq": "^1.2.9",
    "@ai-sdk/openai": "^1.3.24",
    "@ai-sdk/perplexity": "^1.1.9",
    "@ai-sdk/provider": "^2.0.0",
    "@ai-sdk/xai": "^1.2.18",
    "ai": "^5.0.109",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@effect/language-service": "^0.57.1",
    "@effect/vitest": "^0.25.1",
    "@types/node": "^20.19.25",
    "effect": "^3.19.9",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "ultracite": "6.3.6",
    "vitest": "^4.0.15"
  },
  "peerDependencies": {
    "effect": "^3.0.0"
  }
}
